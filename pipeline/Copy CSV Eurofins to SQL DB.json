{
	"name": "Copy CSV Eurofins to SQL DB",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "kompa//439-2023-01240375.csv"
					},
					{
						"name": "Destination",
						"value": "dbo.kompa"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowCopyCommand": true,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "syn-ingraphic-01-WorkspaceDefaultStorage",
							"type": "LinkedServiceReference"
						},
						"path": "kompa"
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "BatchCode",
									"type": "String"
								},
								"sink": {
									"name": "BatchCode",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "SampleCode",
									"type": "String"
								},
								"sink": {
									"name": "SampleCode",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "AR_base_64_coded\r",
									"type": "String"
								},
								"sink": {
									"name": "AR_base_64_coded\r",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSynapseAnalyticsTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}