{
	"name": "JsonToParquet",
	"properties": {
		"folder": {
			"name": "Other"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "JSON",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Parquet",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "syn-ingraphic-01-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "unpivot1"
				},
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Max as double,",
				"          Min as double,",
				"          elementMax as string,",
				"          elementMin as string,",
				"          referenceTime as string,",
				"          sourceId as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine',",
				"     partitionBy('roundRobin', 2)) ~> source1",
				"select1 unpivot(output(",
				"          elementId as string,",
				"          Temperature as double",
				"     ),",
				"     ungroupBy(referenceTime,",
				"          sourceId),",
				"     pivotValues: ['max(air_temperature PT1H)', 'min(air_temperature PT1H)'],",
				"     lateral: false,",
				"     ignoreNullPivots: true) ~> unpivot1",
				"source1 select(mapColumn(",
				"          Max,",
				"          Min,",
				"          referenceTime,",
				"          sourceId",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"cast1 derive(month = month(referenceTime),",
				"          year = year(referenceTime)) ~> derivedColumn1",
				"unpivot1 cast(output(",
				"          referenceTime as timestamp 'yyyy-MM-dd\\'T\\'HH:mm:ss'",
				"     ),",
				"     errors: true) ~> cast1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     truncate: true,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          referenceTime,",
				"          sourceId,",
				"          elementId,",
				"          Temperature,",
				"          month,",
				"          year",
				"     ),",
				"     partitionBy('key',",
				"          0,",
				"          year,",
				"          month",
				"     )) ~> sink1"
			]
		}
	}
}